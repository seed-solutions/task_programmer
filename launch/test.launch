<?xml version="1.0"?>

<launch>

  <arg name="cmd_vel_out" default="cmd_vel"/>
  <arg name="config_locks"  default="$(find twist_mux)/config/twist_mux_locks.yaml"/>
  <arg name="config_topics" default="$(find task_programmer)/config/cmd_vel.config.yaml"/>
  
  <node pkg="twist_mux" type="twist_mux" name="twist_mux" output="screen">
    <remap from="cmd_vel_out" to="$(arg cmd_vel_out)"/>
    <rosparam file="$(arg config_locks)"  command="load"/>
    <rosparam file="$(arg config_topics)" command="load"/>
  </node>  
  
  <!-- twist mux -->
<!--
  <arg name="config_file"           default="$(find task_programmer)/config/cmd_vel.config.yaml"/>
  <include file="$(find twist_mux)/launch/twist_mux.launch">
    <arg name="config_topics"           value="$(arg config_file)"/>
  </include>
-->
</launch>
